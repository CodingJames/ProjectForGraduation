</script>

<script>
    $.datetimepicker.setLocale('th');

    /// DateTimePicker
    $('#datetimepicker').datetimepicker({
        format: 'd/m/Y H:i',
        dayOfWeekStart: 1,
        lang: 'th',
        //disabledDates: [Date.now(), '1986/01/09', '1986/01/10'],
        //startDate:	'1986/01/05'
        startDate: Date.now()
    });
    $('#datetimepicker2').datetimepicker({
        format: 'd/m/Y H:i',
        dayOfWeekStart: 1,
        lang: 'th',
        //disabledDates: ['1986/01/08', '1986/01/09', '1986/01/10'],
        //startDate:	'1986/01/05'
        startDate: Date.now()
    });

</script>
